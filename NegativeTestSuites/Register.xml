<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login and Logout">
    <test name="Login">
        <classes>
            <class name="NegativeTestCases.RegisterTest">
                <methods>
                    <include name="CreateAnAccount"/>
                    <include name="usedEmail"/>
                    <include name="notEnteredFirstName"/>
                    <include name="notEnteredLastName"/>
                    <include name="notEnteredEmail"/>
                    <include name="notEnteredPassword"/>
                    <include name="yourAddress_notEnteredFirstName"/>
                    <include name="yourAddress_NotEnteredLastName"/>
                    <include name="notEnteredFirstAddress"/>
                    <include name="notEnteredCity"/>
                    <include name="notEnteredState"/>
                    <include name="notEnteredPostCode"/>
                    <include name="notSelectedCountry"/>
                    <include name="notEnteredPhone"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>