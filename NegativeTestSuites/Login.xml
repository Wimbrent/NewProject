<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login">
    <test name="Failed login">
        <classes>
            <class name="NegativeTestCases.LoginTest">
                <methods>
                    <include name="notEnteredEmail"/>
                    <include name="notEnteredPassword"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>